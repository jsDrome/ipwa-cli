{
  "name": "jsdrome.com",
  "version": "1.0.0",
  "description": "Mentor driven open source transformation",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "build:client:dev": "webpack --env=client.dev --config=build",
    "build:client:watch": "webpack-dev-server --env=client.dev --config=build",
    "build:client:prod": "webpack --env=client.prod --config=build",
    "build:server:dev": "webpack --env=server.dev --config=build",
    "build:server:watch": "webpack --env=server.dev --config=build",
    "build:server:prod": "webpack --env=server.prod --config=build",
    "start": "nodemon _dist",
    "docker": "docker-compose up --build",
    "dev": "concurrently --kill-others 'npm run build:client:watch' 'npm run build:server:watch' 'npm start' -n client,server,nodemon -c blue,yellow,green"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsDrome/jsdrome.com.git"
  },
  "keywords": [
    "jsdrome",
    "jsdrome.com",
    "mentor",
    "driven",
    "open",
    "source",
    "transformation"
  ],
  "author": "Sreeram Padmanabhan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jsDrome/jsdrome.com/issues"
  },
  "homepage": "https://github.com/jsDrome/jsdrome.com#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^2.0.2",
    "rc": "^1.2.8",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
